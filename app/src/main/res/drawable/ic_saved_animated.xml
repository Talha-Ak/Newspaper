<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group
                android:name="group"
                android:pivotX="12"
                android:pivotY="12">
                <path
                    android:name="path"
                    android:pathData="M 17 3 L 7 3 C 5.9 3 5.01 3.9 5.01 5 L 5 21 L 12 18 L 19 21 L 19 5 C 19 3.9 18.1 3 17 3 Z"
                    android:fillColor="#FF000000"
                    android:strokeColor="#000000"
                    android:strokeAlpha="0"/>
                <path
                    android:name="path_1"
                    android:pathData="M 17 3 L 7 3 C 5.9 3 5.01 3.9 5.01 5 L 5 21 L 12 18 L 19 21 L 19 5 C 19 3.9 18.1 3 17 3 Z M 17 18 L 12 15.82 L 7 18 L 7 5 L 17 5 L 17 18 Z"
                    android:fillColor="#FF000000"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="fillAlpha"
                    android:duration="305"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/linear_out_slow_in"/>
                <objectAnimator
                    android:propertyName="fillAlpha"
                    android:startOffset="305"
                    android:duration="195"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_linear_in"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:duration="200"
                    android:valueFrom="1"
                    android:valueTo="1.3"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/anticipate_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="200"
                    android:valueFrom="1"
                    android:valueTo="1.3"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/anticipate_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:startOffset="200"
                    android:duration="300"
                    android:valueFrom="1.3"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/anticipate_overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="200"
                    android:duration="300"
                    android:valueFrom="1.3"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/anticipate_overshoot_interpolator"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
